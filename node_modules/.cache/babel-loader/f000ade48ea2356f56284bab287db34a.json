           alert('Нет такого пользователя')\n          }\n          this.$refs[formName].resetFields();\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    },\n\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    }\n\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.auth {\n  width: 100%;\n  height: 100vh;\n  font-style: normal;\n  font-weight: 400;\n  background-color: wheat;\n  padding-top: 100px;\n}\n</style>\n"]}]}